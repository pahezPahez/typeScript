  <script>
    const printArrayElements = (arr) => {
      for (let i = 0; i < arr.length; i++) {
        console.log(arr[i]);
      }
    };

    const myArray = [1, 2, 3, 4, 5];

    printArrayElements(myArray);
  </script>
